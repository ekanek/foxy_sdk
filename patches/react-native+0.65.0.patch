+  static dispatch_once_t onceToken;
+  dispatch_once(&onceToken, ^{
+    kRCTAnimatedImageLock = [[NSLock alloc] init];
+  });
+  [kRCTAnimatedImageLock lock];
   CGImageRef imageRef = CGImageSourceCreateImageAtIndex(_imageSource, index, NULL);
   if (!imageRef) {
