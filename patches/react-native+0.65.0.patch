   }
   UIImage *image = [[UIImage alloc] initWithCGImage:imageRef scale:_scale orientation:UIImageOrientationUp];
   CGImageRelease(imageRef);
+  [kRCTAnimatedImageLock unlock];
   return image;
 }
 
